[core]
  editor = nvim
  fsmonitor = true
  untrackedCache = true
  excludesfile = /Users/elizabeth.pine/.gitignore
[column]
  ui = auto
[color]
  ui = auto
[delta]
  features = side-by-side line-numbers decorations
  syntax-theme = OneHalfDark
[alias]
  am = commit --amend
  ap = add -p
  s = status
  c = commit
  co = checkout
  cp = cherry-pick
  gs = status
  gp = pull
  gp = push
  gpm = push origin main
  hp = "!. ~/.githelpers && show_git_head"
  l = "!. ~/.githelpers && pretty_git_log"
  b = "!. ~/.githelpers && pretty_git_branch"
  bs = "!. ~/.githelpers && pretty_git_branch_sorted"
  ski = !git stash --keep-index
[merge]
  tool = vimdiff3
[diff]
  algorithm = histogram
  colorMoved = zebra
  mnemonicPrefix = true
  renames = true
  wordRegex = .
  context = 3
[difftool]
  prompt = false
  trustExitCode = true
[init]
  defaultBranch = main
[push]
  default = simple
  autoSetupRemote = true
  followTags = true
[pull]
  rebase = true
[merge]
  conflictstyle = zdiff3
  tool = vimdiff3
[status]
  short = true
  branch = true
[fetch]
  prune = true
[user]
  name = Liz
  email = lizpny@pm.me
